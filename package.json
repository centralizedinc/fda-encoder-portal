{
  "name": "fda-encoder-portal",
  "version": "0.1.74",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-heroku": "vue-cli-service serve --mode heroku",
    "serve-pcf": "vue-cli-service serve --mode pcf",
    "build-heroku": "vue-cli-service build --mode heroku",
    "build-pcf": "vue-cli-service build --mode pcf",
    "build": "npm run build-heroku",
    "dev": "npm run serve-heroku",
    "start": "node server.js",
    "clean": "if [ -d app]; then rm -r app; fi",
    "pre-deploy": "npm run clean && mkdir app && cp -R dist app/dist && cp -R node_modules app/node_modules && cp package.json app && cp server.js app && cd app",
    "deploy-pcf": "npm run build-pcf && npm run pre-deploy && cf push fda-encoder-portal -m 128M -k 128M",
    "test": "mocha --timeout 10000",
    "push": "git add . && git commit -m 'daily build version' && npm version patch &&  git push origin master"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "fda-pdf-printer-plugin": "^1.0.11",
    "js-cookie": "^2.2.0",
    "material-design-icons-iconfont": "^3.0.3",
    "nprogress": "^0.2.0",
    "pdfmake": "^0.1.51",
    "register-service-worker": "^1.5.2",
    "roboto-fontface": "*",
    "serve-static": "^1.13.2",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.22",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.22",
    "vuetify-loader": "^1.1.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "mocha": "^2.4.5"
  }
}
